{
  "name": "pcb-xpress-website",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "preview:prod": "NODE_ENV=production vite preview --mode production",
    "server": "node server/src/index.js",
    "server:prod": "NODE_ENV=production node server/src/index.js",
    "dev:all": "concurrently \"npm:server\" \"npm:dev\"",
    "prod:all": "concurrently \"npm:server:prod\" \"npm:preview:prod\"",
    "db:seed:products": "node server/scripts/seed-products.js",
    "db:seed:admin-settings": "node server/scripts/seed-admin-settings.js",
    "db:seed:mfg": "node server/scripts/seed-mfg-work-orders.js",
    "db:seed:mfg-operators": "node server/scripts/seed-mfg-operators.js",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^10.16.4",
    "jspdf": "^3.0.3",
    "lucide-react": "0.292.0",
    "mongoose": "^8.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "6.16.0",
    "recharts": "^3.2.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0",
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "terser": "^5.39.0",
    "vite": "^4.4.5"
  }
}
